<beanio xmlns="http://www.beanio.org/2012/03"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.beanio.org/2012/03 http://www.beanio.org/2012/03/mapping.xsd">

    <typeHandler type="java.lang.Integer"
                 class="br.com.dhan.validacaocnab.infra.configuration.beanio.handlers.IntegerCustomTypeHandler"/>

    <typeHandler type="java.math.BigDecimal"
                 class="br.com.dhan.validacaocnab.infra.configuration.beanio.handlers.BigDecimalCustomTypeHandler"/>

    <typeHandler name="cpfCnpjAttTypeHandler"
                 class="br.com.dhan.validacaocnab.infra.configuration.beanio.handlers.CpfCnpjAttTypeHandler"/>

    <typeHandler name="bigDecimalConvertTypeHandler"
                 class="br.com.dhan.validacaocnab.infra.configuration.beanio.handlers.BigDecimalConvertTypeHandler"/>

    <typeHandler type="java.time.LocalDate"
                 class="br.com.dhan.validacaocnab.infra.configuration.beanio.handlers.LocalDateTypeHandler"/>

</beanio>


